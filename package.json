{
  "name": "typelings",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexander Skvorchevskiy <alex.skvorchevskiy@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "eslint": "^9.16.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.13.0",
    "prettier": "3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
